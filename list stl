int main()
{
  list<int>l ;
  list<int> n (5 , 100) ;
  l.push_back(5) ;
  l.push_front(4) ;
  for(int i : l)
  {
      cout << i << " " ;
  }
  cout<< endl ;
  cout << "Before deletion size is " << l.size() << endl ;
  l.erase(l.begin()) ;
cout <<"After deletion size will be " << l.size() << endl ;
for(int i : n)
{
    cout << i  << " " ;
}
    return 0;
}
